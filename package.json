{
  "name": "tigerbeetle-node",
  "version": "0.2.0",
  "description": "Official TigerBeetle Node.js client",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    ".gitmodules",
    "LICENSE",
    "README.md",
    "package.json",
    "tsconfig.json",
    "yarn.lock",
    "src/index.ts",
    "src/node.zig",
    "scripts/get_node_headers"
  ],
  "bin": {
    "zig": "./bin/zig"
  },
  "scripts": {
    "build": "tsc && zig build-lib -dynamic -lc -isystem ./node-$(node --version)/include/node src/node.zig -femit-bin=dist/client.node",
    "clean": "rm -rf ./node_modules ./dist ./.zig ./.bin ./build",
    "install:tigerbeetle": "git submodule init && git submodule update",
    "install:zig": "./src/tigerbeetle/scripts/install_zig tagged",
    "preinstall": "yarn install:tigerbeetle && yarn install:zig && ./scripts/get_node_headers",
    "postinstall": "yarn build"
  },
  "author": "Coil Technologies, Inc",
  "license": "Apache-2.0",
  "contributors": [
    "Donovan Changfoot <don@coil.com>",
    "Isaac Freund <isaac.freund@coil.com>",
    "Joran Dirk Greef <joran@coil.com>"
  ],
  "devDependencies": {
    "@types/node": "^14.14.41",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
