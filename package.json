{
  "name": "tigerbeetle-node",
  "version": "0.2.0",
  "description": "Official TigerBeetle Node.js client",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    ".gitmodules",
    "LICENSE",
    "README.md",
    "package.json",
    "tsconfig.json",
    "yarn.lock",
    "src/index.ts",
    "src/node.zig",
    "scripts/download_node_headers.sh"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc && zig/zig build-lib -dynamic -lc -isystem build/node-$(node --version)/include/node src/node.zig -femit-bin=dist/client.node",
    "clean": "rm -rf build dist node_modules src/zig-cache zig",
    "install:tigerbeetle": "git submodule init && git submodule update",
    "install:zig": "./src/tigerbeetle/scripts/install_zig.sh",
    "preinstall": "yarn install:tigerbeetle && yarn install:zig && ./scripts/download_node_headers.sh",
    "postinstall": "yarn build"
  },
  "author": "Coil Technologies, Inc",
  "license": "Apache-2.0",
  "contributors": [
    "Donovan Changfoot <don@coil.com>",
    "Isaac Freund <isaac.freund@coil.com>",
    "Joran Dirk Greef <joran@coil.com>"
  ],
  "devDependencies": {
    "@types/node": "^14.14.41",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
